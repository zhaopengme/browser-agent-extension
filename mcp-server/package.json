{
  "name": "browser-agent-extension-mcp",
  "version": "1.0.4",
  "description": "MCP Server for Browser Agent Extension - Control Chrome browser via AI",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "browser-agent-extension-mcp": "dist/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun src/main.ts",
    "build": "bun build src/main.ts --outdir dist",
    "build:bin": "bun build --compile src/main.ts --outfile dist/browser-agent-mcp",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "start": "bun dist/main.js",
    "test": "bun test",
    "prepublishOnly": "npm run build && npm run build:types"
  },
  "keywords": [
    "mcp",
    "browser",
    "automation",
    "chrome",
    "extension",
    "ai",
    "claude"
  ],
  "author": "agents-cc <service.agents.cc@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agents-cc/browser-agent-extension.git"
  },
  "homepage": "https://github.com/agents-cc/browser-agent-extension#readme",
  "bugs": {
    "url": "https://github.com/agents-cc/browser-agent-extension/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/ws": "^8.5.13",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
